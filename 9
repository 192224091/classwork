To write a C code to implement Nested Structure concept  
#include <stdio.h>

// Define a nested structure
struct Address {
    char street[50];
    char city[50];
    int zip;
};

struct Person {
    char name[50];
    int age;
    struct Address address;
};

int main() {
    // Declare and initialize a nested structure
    struct Person person1 = {
        "John Smith",
        35,
        {
            "123 Main St",
            "Anytown",
            12345
        }
    };
    
    // Print the values of the nested structure
    printf("Name: %s\n", person1.name);
    printf("Age: %d\n", person1.age);
    printf("Street: %s\n", person1.address.street);
    printf("City: %s\n", person1.address.city);
    printf("Zip: %d\n", person1.address.zip);
    
    return 0;
}
