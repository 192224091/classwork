Program to swap two #include <iostream>
using namespace std;

// Function to swap two numbers
void swap(int a, int b) {
    int temp = a;
    a = b;
    b = temp;
}

int main() {
    int x = 10, y = 20;
    cout << "Before swapping: " << "x = " << x << ", y = " << y << endl;

    // Call the swap function
    swap(x, y);

    cout << "After swapping: " << "x = " << x << ", y = " << y << endl;

    return 0;
}
 using call by value
